{
    "name": "robrogers3/laracastle",
    "description": "A Castle.io integration package for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Rogers",
            "email": "robrogers@me.com",
            "homepage": "https://github.com/robrogers3"
        }
    ],
    "homepage": "https://github.com/robrogers3/laracastle",
    "keywords": ["Laravel", "Laracastle"],
    "require": {
        "illuminate/support": "~5|~6",
        "castle/castle-php": "^2.0",
        "kitetail/zttp": "^0.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "robrogers3\\Laracastle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "robrogers3\\Laracastle\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "robrogers3\\Laracastle\\LaracastleServiceProvider"
            ],
            "aliases": {
                "Laracastle": "robrogers3\\Laracastle\\Facades\\Laracastle"
            }
        }
    }
}
